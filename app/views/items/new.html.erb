<nav>
  <div>
    <%= link_to image_tag("https://img.icons8.com/ios-filled/48/ffffff/long-arrow-left.png"), group_items_path, class: "btn"  %>
  </div>
  <h1>NEW TRANSACTIONS</h1>
  <img src="https://img.icons8.com/sf-black/48/ffffff/search.png"/>
</nav>
<div class="center">
  <h1>ADD A NEW TRANSACTION</h1>
  <%= form_with model: @group_item, url: group_items_path do |f| %>
      <br/>
      <br/>
      <%= f.text_field :name, placeholder: 'Name', class: "form-field"%>
      <br/>
      <br/>
      <%= f.number_field :amount, placeholder: 'Amount', class: "form-field"%>
      <br/>
      <br/>
      <%= f.submit 'Add Transaction', class:'green-btn'%>
  <% end %>
</div>